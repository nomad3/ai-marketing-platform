# Environment variables for MCP Server

# Meta Marketing API
META_APP_ID=your_meta_app_id
META_APP_SECRET=your_meta_app_secret
META_ACCESS_TOKEN=your_meta_access_token

# AI Content Generation Services
HUGGINGFACE_API_KEY=your_huggingface_api_key
OPENAI_API_KEY=your_openai_api_key

# Video Generation (example: Runway, Pika, etc.)
VIDEO_GEN_API_KEY=your_video_gen_api_key
VIDEO_GEN_API_URL=https://api.videoservice.com

# Higgsfield AI
HIGGSFIELD_API_KEY_ID=your_higgsfield_api_key_id
HIGGSFIELD_API_KEY_SECRET=your_higgsfield_api_key_secret

# Database
DATABASE_URL=postgresql://postgres:postgres@postgres:5432/marketing_platform
REDIS_URL=redis://redis:6379

# Server Configuration
NODE_ENV=development
PORT=3001
LOG_LEVEL=info
